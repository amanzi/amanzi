CHEMISTRY
  PRIMARY_SPECIES
    A
    B
    C
  /
  SORPTION
    ISOTHERM_REACTIONS
      A
        TYPE LINEAR
        DISTRIBUTION_COEFFICIENT 10
      /
      B
        TYPE LANGMUIR
        DISTRIBUTION_COEFFICIENT 30
        LANGMUIR_B 0.1
      /
      C
        TYPE FREUNDLICH
        DISTRIBUTION_COEFFICIENT 1.5
        FREUNDLICH_N 0.8
      /
    /
  /
  DATABASE isotherms.dat
  ACTIVITY_COEFFICIENTS TIMESTEP
  LOG_FORMULATION 
  USE_FULL_GEOCHEMISTRY 
END
CONSTRAINT initial
  CONCENTRATIONS
    A 1e-20 T
    B 1e-20 T
    C 1e-20 T
  /
END
CONSTRAINT west
  CONCENTRATIONS
    A 0.001 T
    B 0.001 T
    C 0.001 T
  /
END
