CHEMISTRY
  PRIMARY_SPECIES
    H+
    Na+
    NO3-
    Zn++
  /
  SECONDARY_SPECIES
    OH-
    Zn(OH)2(aq)
    Zn(OH)3-
    Zn(OH)4--
    ZnOH+
  /
  SORPTION
    SURFACE_COMPLEXATION_RXN
      EQUILIBRIUM
      SITE >FeOH_s 0.001908
      COMPLEXES
        >FeOHZn+_s
        >FeOH2+_s
        >FeO-_s
      /
    /
    SURFACE_COMPLEXATION_RXN
      EQUILIBRIUM
      SITE >FeOH_w 0.076355
      COMPLEXES
        >FeOHZn+_w
        >FeO-_w
        >FeOH2+_w
      /
    /
  /
  DATABASE surface-complexation.dat
  ACTIVITY_COEFFICIENTS TIMESTEP
  LOG_FORMULATION 
  USE_FULL_GEOCHEMISTRY 
END
CONSTRAINT initial
  CONCENTRATIONS
    Zn++ 1e-07 T
    NO3- 0.1 T
    H+ 5 P
    Na+ 1e-05 Z
  /
END
CONSTRAINT west
  CONCENTRATIONS
    Zn++ 1e-06 T
    NO3- 0.1 T
    H+ 6 P
    Na+ 1e-05 Z
  /
END
CONSTRAINT east
  CONCENTRATIONS
    Zn++ 1e-07 T
    NO3- 0.1 T
    H+ 5 P
    Na+ 1e-05 Z
  /
END
