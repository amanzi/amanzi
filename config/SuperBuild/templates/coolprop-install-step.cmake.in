# CMake COOLPROP configure file

# Now run the patch script
execute_process(
  COMMAND sh @COOLPROP_sh_install@
  RESULT_VARIABLE result
  WORKING_DIRECTORY "@COOLPROP_source_dir@"
)

if (result)
  set(msg "COOLPROP install command failed")
  foreach(arg IN LISTS command )
    set(msg "${msg} '${arg}'")
  endforeach()
  message(FATAL_ERROR ${msg})
endif()  

                            
