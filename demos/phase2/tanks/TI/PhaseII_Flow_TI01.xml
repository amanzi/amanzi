        <Parameter name="Value" type="double" value="0.266"/>
      </ParameterList>
      <ParameterList name="Intrinsic Permeability: Anisotropic Uniform">
        <Parameter name="x" type="double" value="3.70e-17"/>
        <Parameter name="y" type="double" value="3.70e-17"/>
