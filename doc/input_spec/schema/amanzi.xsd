		<xs:enumeration value="warnings"/>
		<xs:enumeration value="errors"/>
	</xs:restriction>
</xs:simpleType>

<xs:complexType name="typedef_geochem_constraints">
	<xs:all minOccurs="0" maxOccurs="1">
