</xs:complexType>

<xs:complexType name="typedef_gases">
	<xs:all minOccurs="0" maxOccurs="1">
		<xs:element name="gas" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
	</xs:all>
</xs:complexType>


