		<xs:element name="max_iterations" type="xs:integer" minOccurs="0" maxOccurs="1"/>
		<xs:element name="tolerance" type="typedef_exponential" minOccurs="0" maxOccurs="1"/>
        	<xs:element name="preconditioner" type="xs:string" minOccurs="0" maxOccurs="1"/>
	</xs:all>
	<xs:assert test="not(exists(method)) or method = ('gmres','pcg')"/>
	<xs:assert test="not(exists(preconditioner)) or preconditioner = ('hypre_amg','trilinos_ml','block_ilu')"/>
