<xs:complexType name="typedef_misc">
	<xs:all minOccurs="0" maxOccurs="1">
		<xs:element name="echo_translated_input" type="typedef_translated_input" minOccurs="0" maxOccurs="1"/>
	</xs:all>
</xs:complexType>

<xs:complexType name="typedef_translated_input">
	<xs:attribute type="typedef_native_format" name="format"/>
	<xs:attribute type="xs:string" name="file_name"/>
	<xs:attribute type="xs:nonNegativeInteger" name="output_precision"/>
</xs:complexType>

<xs:simpleType name="typedef_native_format">
	<xs:restriction base="xs:string">
		<xs:enumeration value="unstructured_native"/>
		<xs:enumeration value="structured_native"/>
	</xs:restriction>
</xs:simpleType>

<xs:simpleType name="typedef_coordinate_system">
	<xs:restriction base="xs:string">	
		<xs:pattern value="[xyz](,y|,z|,y,z)"/>
	</xs:restriction>
</xs:simpleType>

