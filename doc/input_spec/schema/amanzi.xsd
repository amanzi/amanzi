	</xs:all>
</xs:complexType>

<xs:complexType name="typedef_geochem_constraint">
	<!-- TODO: at least one solute or mineral is required if filename is not given -->
	<xs:all minOccurs="0" maxOccurs="1">
		<xs:element name="primary" type="typedef_geochem_primary" minOccurs="0" maxOccurs="unbounded"/>
		<xs:element name="mineral" type="typedef_mineral_properties" minOccurs="0" maxOccurs="unbounded"/>
