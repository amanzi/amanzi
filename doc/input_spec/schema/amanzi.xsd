<xs:complexType name="typedef_observation">
	<xs:all>
		<xs:element name="assigned_regions" type="typedef_assigned_regions" minOccurs="1" maxOccurs="1"/>
		<xs:element name="functional" type="xs:string" minOccurs="1" maxOccurs="1"/>
		<xs:element name="time_macros" type="xs:string" minOccurs="1" maxOccurs="unbounded"/>
	</xs:all>
	<xs:assert test="not(exists(functional)) or functional = ('point', 'integral')"/>
</xs:complexType>

<xs:complexType name="typedef_primary_observation">
	<xs:all>
		<xs:element name="assigned_regions" type="typedef_assigned_regions" minOccurs="1" maxOccurs="1"/>
		<xs:element name="functional" type="xs:string" minOccurs="1" maxOccurs="1"/>
		<xs:element name="time_macros" type="xs:string" minOccurs="1" maxOccurs="unbounded"/>
	</xs:all>
	<xs:attribute name="solute"/>
	<xs:assert test="not(exists(functional)) or functional = ('point', 'integral')"/>
</xs:complexType>


