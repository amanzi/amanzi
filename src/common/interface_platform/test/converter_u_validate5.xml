        <Parameter name="tolerance" type="double" value="0."/>
        <Parameter name="smoother sweeps" type="int" value="3"/>
        <Parameter name="cycle applications" type="int" value="5"/>
        <Parameter name="strong threshold" type="double" value="0.5"/>
