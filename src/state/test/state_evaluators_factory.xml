<ParameterList name="main">

  <ParameterList name="mesh">
    <Parameter name="number of cells" type="Array(int)" value="{2,1}" />
    <Parameter name="domain low coordinate" type="Array(double)" value="{0.,0.}" />
    <Parameter name="domain high coordinate" type="Array(double)" value="{4.,1.}" />
  </ParameterList>
  
  <ParameterList name="regions">
    <ParameterList name="all">
      <ParameterList name="region: all">
      </ParameterList>
    </ParameterList>
    <ParameterList name="left">
      <ParameterList name="region: box">
        <Parameter name="low coordinate" type="Array(double)" value="{0.,0.}" />
        <Parameter name="high coordinate" type="Array(double)" value="{2.,1.}" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="right">
      <ParameterList name="region: box">
        <Parameter name="low coordinate" type="Array(double)" value="{2.,0.}" />
        <Parameter name="high coordinate" type="Array(double)" value="{4.,1.}" />
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <ParameterList name="state">
    
    <ParameterList name="evaluators">

      <ParameterList name="saturation_liquid">
        <Parameter name="evaluator type" type="string" value="water retention van Genuchten by material" />
        <Parameter name="capillary pressure key" type="string" value="capillary_pressure_gas_liquid" />  <!-- default, not actually needed -->
        <Parameter name="common model parameters" type="string" value="water retention van Genuchten by material" />
      </ParameterList>

      <ParameterList name="molar_density_liquid">
        <Parameter name="evaluator type" type="string" value="independent variable constant value" />
        <Parameter name="value" type="double" value="55000." />
      </ParameterList>

      <ParameterList name="porosity">
        <Parameter name="evaluator type" type="string" value="compressible porosity" />
        <Parameter name="pressure key" type="string" value="pressure" /> <!-- default, not actually needed -->
        <Parameter name="base porosity key" type="string" value="base_porosity" />  <!-- default, not actually needed -->
        <ParameterList name="model parameters">
          <Parameter name="pore compressibility [Pa^-1]" type="double" value="1e-07" />
        </ParameterList>
      </ParameterList>
      
      <ParameterList name="base_porosity">
        <Parameter name="evaluator type" type="string" value="independent variable" />
        <ParameterList name="function" type="ParameterList">
          <ParameterList name="left" type="ParameterList">
            <ParameterList name="function" type="ParameterList">
              <ParameterList name="function-constant" type="ParameterList">
                <Parameter name="value" type="double" value="0.5" />
              </ParameterList>
            </ParameterList>
          </ParameterList>
          <ParameterList name="right" type="ParameterList">
            <ParameterList name="function" type="ParameterList">
              <ParameterList name="function-constant" type="ParameterList">
                <Parameter name="value" type="double" value="0.25" />
              </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>

      <ParameterList name="pressure">
        <Parameter name="evaluator type" type="string" value="independent variable constant value" />
        <Parameter name="value" type="double" value="90000." />
      </ParameterList>

      <ParameterList name="capillary_pressure_gas_liquid">
        <Parameter name="evaluator type" type="string" value="secondary variable from function" />
        <Parameter name="dependencies" type="Array(string)" value="{pressure}" />
        <ParameterList name="function">
          <ParameterList name="function-linear">
            <Parameter name="y0" type="double" value="101325." />
            <Parameter name="gradient" type="Array(double)" value="{-1.0}" />
            <Parameter name="x0" type="Array(double)" value="{0.0}" />
          </ParameterList>
        </ParameterList>
        <ParameterList name="derivative functions">
          <ParameterList name="pressure">
            <ParameterList name="function-constant">
              <Parameter name="value" type="double" value="-1" />
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>

      
      <ParameterList name="water_content">
        <Parameter name="evaluator type" type="string" value="multiplicative" />
        <Parameter name="dependencies" type="Array(string)" value="{saturation_liquid, molar_density_liquid, porosity}" />
      </ParameterList>
      
    </ParameterList>
    
    <ParameterList name="common model parameters">

      <ParameterList name="water retention van Genuchten by material">
        <ParameterList name="left" type="ParameterList">
          <Parameter name="alpha [Pa^-1]" type="double" value="0.002" />
          <Parameter name="n [-]" type="double" value="2.0" />
          <Parameter name="residual saturation [-]" type="double" value="0.0" />
        </ParameterList>
        <ParameterList name="right" type="ParameterList">
          <Parameter name="alpha [Pa^-1]" type="double" value="0.00015" />
          <Parameter name="n [-]" type="double" value="1.6" />
          <Parameter name="residual saturation [-]" type="double" value="0.1" />
        </ParameterList>
      </ParameterList>

    </ParameterList>
  </ParameterList>

</ParameterList>

  
