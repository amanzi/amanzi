    <flow model="richards" state="on" />
    <transport state="off" />
    <chemistry engine="none" state="off" />
  </process_kernels>

  <phases>
    <liquid_phase name="water">
    <eos>false</eos>
    <viscosity>1.002E-03</viscosity>
    <density>998.2</density>
