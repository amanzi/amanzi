<amanzi_input type="unstructured" version="2.3.0">
  <model_description name="SR Test Problem">
    <comments>Confined aquifer transient drawdown (Butler and Liu, 1991)</comments>
    <model_id>Butler</model_id>
    <author>Dylan Harp, Zhiming Lu</author>
    <units>
      <length_unit>m</length_unit>
      <time_unit>s</time_unit>
      <mass_unit>kg</mass_unit>
      <conc_unit>molar</conc_unit>
    </units>
  </model_description>
  <definitions>
    <macros>
      <time_macro name="Observation Times">
        <time>0.0</time>
        <time>1</time>
        <time>3</time>
        <time>7</time>
        <time>15</time>
        <time>31</time>
        <time>63</time>
        <time>127</time>
        <time>255</time>
        <time>511</time>
        <time>1023</time>
        <time>2047</time>
        <time>4095</time>
        <time>8191</time>
        <time>16383</time>
        <time>32677</time>
        <time>65535</time>
        <time>131071</time>
        <time>262143</time>
        <time>524287</time>
        <time>1e6</time>
        <time>1e7</time>
        <time>1e8</time>
        <time>1e9</time>
      </time_macro>
    </macros>
  </definitions>

  <process_kernels>
    <comments>Single-phase transient flow</comments>
    <flow model="saturated" state="on" />
    <transport state="off" />
    <chemistry engine="none" state="off" />
  </process_kernels>

  <execution_controls>
    <verbosity level="high" />
    <execution_control_defaults increase_factor="1.5" init_dt="3.64" max_dt="1.64e7" method="bdf1" />
    <execution_control end="1.64e7" init_dt="3.64" mode="transient" start="0.0" />
  </execution_controls>

  <numerical_controls>
    <unstructured_controls>
      <unstr_linear_solver>
        <method>pcg</method>
        <max_iterations>100</max_iterations>
        <tolerance>1.0e-16</tolerance>
      </unstr_linear_solver>
      <unstr_transport_controls>
        <algorithm>none</algorithm>
      </unstr_transport_controls>
      <unstr_preconditioners>
        <hypre_amg />
        <trilinos_ml />
        <block_ilu />
      </unstr_preconditioners>
    </unstructured_controls>
  </numerical_controls>

  <mesh framework="mstk">
    <comments>Three-dimensional box 100m x 50m x 50m</comments>
    <dimension>3</dimension>
    <generate>
      <number_of_cells nx="601" ny="601" nz="1" />
      <box high_coordinates="1202.0,1202.0,1.0" low_coordinates="-1202.0,-1202.0,0.0" />
    </generate>
  </mesh>

  <regions>
    <region name="Entire Domain">
      <comments>One region covering the entire domain</comments>
      <box high_coordinates="1202.0,1202.0,1.0" low_coordinates="-1202.0,-1202.0,0.0" />
    </region>
    <region name="Zone_1">
      <comments>The left zone</comments>
      <box high_coordinates="-10.0,1202.0,1.0" low_coordinates="-1202.0,-1202.0,0.0" />
    </region>
    <region name="Zone_2">
      <comments>The strip</comments>
      <box high_coordinates="10.0,1202.0,1.0" low_coordinates="-10.0,-1202.0,0.0" />
    </region>
    <region name="Zone_3">
      <comments>The right zone</comments>
      <box high_coordinates="1202.0,1202.0,1.0" low_coordinates="10.0,-1202.0,0.0" />
    </region>
    <region name="Well">
      <comments>Pumping Well</comments>
      <box high_coordinates="1.0,1.0,1.0" low_coordinates="-1.0,-1.0,0.0" />
    </region>
    <region name="yz1">
      <comments>Left Plane</comments>
      <plane location="-1202.0,-1202.0,0.0" normal="-1.0,0.0,0.0" />
    </region>
    <plane location="1202.0,-1202.0,0.0" name="yz2" normal="1.0,0.0,0.0" />
    <plane location="-1202.0,-1202.0,0.0" name="xz1" normal="0.0,-1.0,0.0" />
    <plane location="-1202.0,1202.0,0.0" name="xz2" normal="0.0,1.0,0.0" />
    <point coordinate="24.0,0.0,1.0" name="Obs_r24" />
    <point coordinate="100.0,0.0,1.0" name="Obs_r100" />
  </regions>

  <phases>
    <liquid_phase name="water">
      <eos>false</eos>
      <viscosity>1.002E-03</viscosity>
      <density>1000.0</density>
    </liquid_phase>
  </phases>

  <materials>
    <material name="Soil_1">
      <comments>Material properties for zone 1</comments>
      <assigned_regions>Zone_1</assigned_regions>
      <mechanical_properties>
        <porosity value="0.25" />
        <specific_storage model="constant" value="5.0e-4" />
      </mechanical_properties>
      <permeability x="1.187E-8" y="1.187E-8" z="1.187E-8" />
    </material>
    <material name="Soil_2">
      <comments>Material properties for zone 2</comments>
      <assigned_regions>Zone_2</assigned_regions>
      <mechanical_properties>
        <porosity value="0.25" />
        <specific_storage model="constant" value="2.0e-4" />
      </mechanical_properties>
      <permeability x="1.187E-9" y="1.187E-9" z="1.187E-9" />
    </material>
    <material name="Soil_3">
      <comments>Material properties for zone 3</comments>
      <assigned_regions>Zone_3</assigned_regions>
      <mechanical_properties>
        <porosity value="0.25" />
        <specific_storage model="constant" value="2.0e-5" />
      </mechanical_properties>
      <permeability x="1.187E-10" y="1.187E-10" z="1.187E-10" />
    </material>
  </materials>

  <initial_conditions>
    <initial_condition name="Initial Condition">
      <comments>Constant pressure of 100m</comments>
      <assigned_regions>Entire Domain</assigned_regions>
      <liquid_phase name="water">
        <liquid_component name="water">
          <linear_pressure gradient="0.0,0.0,0" reference_coord="1202.0,1202.0,10.0" value="1.07785e6" />
        </liquid_component>
      </liquid_phase>
    </initial_condition>
  </initial_conditions>

  <boundary_conditions>
    <boundary_condition name="Far Field Head">
      <comments>Far field static head of the confined aquifer</comments>
      <assigned_regions>xz1,xz2,yz1,yz2</assigned_regions>
      <liquid_phase name="water">
        <liquid_component name="water">
          <uniform_pressure function="constant" start="1.07785e6" value="1.07785e6" />
        </liquid_component>
      </liquid_phase>
    </boundary_condition>
  </boundary_conditions>

  <sources>
    <source name="Pumping Well">
      <assigned_regions>Well</assigned_regions>
      <liquid_phase name="water">
        <liquid_component name="water">
          <volume_weighted function="constant" start="0.0" value="-11.5485" />
        </liquid_component>
      </liquid_phase>
    </source>
  </sources>

  <output>
    <observations>
      <filename>observations.out</filename>
      <liquid_phase name="water">
        <drawdown>
          <assigned_regions>Obs_r24</assigned_regions>
          <functional>point</functional>
          <time_macros>Observation Times</time_macros>
        </drawdown>
        <drawdown>
          <assigned_regions>Obs_r100</assigned_regions>
          <functional>point</functional>
          <time_macros>Observation Times</time_macros>
        </drawdown>
      </liquid_phase>
    </observations>
  </output>
</amanzi_input>
  
