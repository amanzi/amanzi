<ParameterList name="Main">

  <Parameter name="Amanzi Input Format Version" type="string" value="1.2.3"/>

  <!--
     Getting started on a simple 2D steady-state saturated test
  -->
  <ParameterList name="General Description">
    <Parameter name="Model ID" type="string" value="1SSConFlow4"/>
    <Parameter name="Author" type="string" value="Greg Flach, SRNL"/>
  </ParameterList>

  <!--
     Execution Control
  -->
  <ParameterList name="Execution Control">
    <Parameter name="Flow Model" type="string" value="Single Phase"/>
    <Parameter name="Transport Model" type="string" value="Off"/>
    <Parameter name="Chemistry Model" type="string" value="Off"/>

    <ParameterList name="Time Integration Mode">
        <ParameterList name="Steady">
          <Parameter name="Start" type="double" value="0.0"/>
          <Parameter name="End" type="double" value="1000.0"/>
          <Parameter name="Initial Time Step" type="double" value="1.0"/>
        </ParameterList>
    </ParameterList>

    <Parameter name="Verbosity" type="string" value="High"/>

    <ParameterList name="Numerical Control Parameters">
      <ParameterList name="Unstructured Algorithm">
        <ParameterList name="Linear Solver">
          <Parameter name="linear solver tolerance" type="double" value="1e-18"/>
          <Parameter name="linear solver maximum iterations" type="int" value="10"/>
        </ParameterList>
      </ParameterList>
    </ParameterList>
  
  </ParameterList>
      
  <ParameterList name="Domain">
    <Parameter name="Spatial Dimension" type="int" value="3"/>
  </ParameterList>

  <!--
     Mesh:  three-dimensional box 100m x 2m x 10m 
  -->
  <ParameterList name="Mesh">
    <ParameterList name="Unstructured">
      <ParameterList name="Expert">
	<Parameter name="Framework" type="string" value="MSTK"/>
      </ParameterList>
      <ParameterList name="Generate Mesh">
        <ParameterList name="Uniform Structured">
          <Parameter name="Number of Cells" type="Array(int)" value="{20, 2, 1}"/>
          <Parameter name="Domain Low Coordinate" type="Array(double)" value="{0.0, 0.0, 0.0}" />
          <Parameter name="Domain High Coordinate" type="Array(double)" value="{100.0, 2.0, 10.0}" />
        </ParameterList>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <!--
    Regions: 
  -->
  <ParameterList name="Regions">
    <!--
        Domains:
    -->
    <ParameterList name="Entire Domain">
      <ParameterList name="Region: Box">
        <Parameter name="Low Coordinate" type="Array(double)" value="{0.0, 0.0, 0.0}"/>
        <Parameter name="High Coordinate" type="Array(double)" value="{100.0, 2.0, 10.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Front Half">
      <ParameterList name="Region: Box">
        <Parameter name="Low Coordinate" type="Array(double)" value="{0.0, 0.0, 0.0}"/>
        <Parameter name="High Coordinate" type="Array(double)" value="{100.0, 1.0, 10.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Back Half">
      <ParameterList name="Region: Box">
        <Parameter name="Low Coordinate" type="Array(double)" value="{0.0, 1.0, 0.0}"/>
        <Parameter name="High Coordinate" type="Array(double)" value="{100.0, 2.0, 10.0}"/>
      </ParameterList>
    </ParameterList>
    <!--
        Boundaries
    -->
    <ParameterList name="Left">
      <ParameterList name="Region: Box">
        <Parameter name="Low Coordinate" type="Array(double)" value="{0.0, 0.0, 0.0}"/>
        <Parameter name="High Coordinate" type="Array(double)" value="{0.0, 2.0, 10.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Right">
      <ParameterList name="Region: Box">
        <Parameter name="Low Coordinate" type="Array(double)" value="{100.0, 0.0, 0.0}"/>
        <Parameter name="High Coordinate" type="Array(double)" value="{100.0, 2.0, 10.0}"/>
      </ParameterList>
    </ParameterList>
    <!-- 
	 Observation points:
    -->
    <ParameterList name="LeftBoundaryFront">
      <ParameterList name="Region: Point">
	<Parameter name="Coordinate" type="Array(double)" value="{0.0, 0.5, 5.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="RightBoundaryFront">
      <ParameterList name="Region: Point">
	<Parameter name="Coordinate" type="Array(double)" value="{100.0, 0.5, 5.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="MidpointFront">
      <ParameterList name="Region: Point">
	<Parameter name="Coordinate" type="Array(double)" value="{50.0, 0.5, 5.0}"/>
      </ParameterList>
    </ParameterList>

    <ParameterList name="LeftmostCellFront">
      <ParameterList name="Region: Point">
	<Parameter name="Coordinate" type="Array(double)" value="{2.5, 0.5, 5.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="RightmostCellFront">
      <ParameterList name="Region: Point">
	<Parameter name="Coordinate" type="Array(double)" value="{97.5, 0.5, 5.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="LeftMidpointCellFront">
      <ParameterList name="Region: Point">
	<Parameter name="Coordinate" type="Array(double)" value="{47.5, 0.5, 5.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="RightMidpointCellFront">
      <ParameterList name="Region: Point">
	<Parameter name="Coordinate" type="Array(double)" value="{52.5, 0.5, 5.0}"/>
      </ParameterList>
    </ParameterList>

    <ParameterList name="LeftBoundaryBack">
      <ParameterList name="Region: Point">
	<Parameter name="Coordinate" type="Array(double)" value="{0.0, 1.5, 5.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="RightBoundaryBack">
      <ParameterList name="Region: Point">
	<Parameter name="Coordinate" type="Array(double)" value="{100.0, 1.5, 5.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="MidpointBack">
      <ParameterList name="Region: Point">
	<Parameter name="Coordinate" type="Array(double)" value="{50.0, 1.5, 5.0}"/>
      </ParameterList>
    </ParameterList>

    <ParameterList name="LeftmostCellBack">
      <ParameterList name="Region: Point">
	<Parameter name="Coordinate" type="Array(double)" value="{2.5, 1.5, 5.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="RightmostCellBack">
      <ParameterList name="Region: Point">
	<Parameter name="Coordinate" type="Array(double)" value="{97.5, 1.5, 5.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="LeftMidpointCellBack">
      <ParameterList name="Region: Point">
	<Parameter name="Coordinate" type="Array(double)" value="{47.5, 1.5, 5.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="RightMidpointCellBack">
      <ParameterList name="Region: Point">
	<Parameter name="Coordinate" type="Array(double)" value="{52.5, 1.5, 5.0}"/>
      </ParameterList>
    </ParameterList>

  </ParameterList>

  <!--
    Materials:
  -->
  <ParameterList name="Material Properties">

    <ParameterList name="Front Material">
      <Parameter name="Assigned Regions" type="Array(string)" value="{Front Half}"/>
      <ParameterList name="Porosity: Uniform">
        <Parameter name="Value" type="double" value="0.25"/>
      </ParameterList>
      <ParameterList name="Intrinsic Permeability: Uniform">
        <Parameter name="Value" type="double" value="1.1847e-12"/>
      </ParameterList>
    </ParameterList>

    <ParameterList name="Back Material">
      <Parameter name="Assigned Regions" type="Array(string)" value="{Back Half}"/>
      <ParameterList name="Porosity: Uniform">
        <Parameter name="Value" type="double" value="0.25"/>
      </ParameterList>
      <ParameterList name="Intrinsic Permeability: Uniform">
        <Parameter name="Value" type="double" value="1.1847e-11"/>
      </ParameterList>
    </ParameterList>

  </ParameterList>

  <!--
    Phases:
  -->
  <ParameterList name="Phase Definitions">
    <ParameterList name="Aqueous">
      <ParameterList name="Phase Properties">
        <ParameterList name="Viscosity: Uniform">
          <Parameter name="Viscosity" type="double" value="1.002e-3"/>
        </ParameterList>
        <ParameterList name="Density: Uniform">
          <Parameter name="Density" type="double" value="998.2"/>
        </ParameterList>
      </ParameterList>

    </ParameterList>
  </ParameterList>

  <!--
    Initial Conditions:
  -->
  <ParameterList name="Initial Conditions">

<!--
    <ParameterList name="Initial Condition">
      <Parameter name="Assigned Regions" type="Array(string)" value="{Entire Domain}"/>
      <ParameterList name="IC: Uniform Pressure">
        <Parameter name="Phase" type="string" value="Aqueous"/>
        <Parameter name="Value" type="double" value="190892.5"/>
      </ParameterList>
    </ParameterList>
-->
    <ParameterList name="Left IC">
      <Parameter name="Assigned Regions" type="Array(string)" value="{Front Half}"/>
      <ParameterList name="IC: Uniform Pressure">
        <Parameter name="Phase" type="string" value="Aqueous"/>
        <Parameter name="Value" type="double" value="190892.5"/>
      </ParameterList>
    </ParameterList>

    <ParameterList name="Right IC">
      <Parameter name="Assigned Regions" type="Array(string)" value="{Back Half}"/>
      <ParameterList name="IC: Uniform Pressure">
        <Parameter name="Phase" type="string" value="Aqueous"/>
        <Parameter name="Value" type="double" value="190892.5"/>
      </ParameterList>
    </ParameterList>

  </ParameterList>

  <!--
    Boundary Conditions
  -->
  <ParameterList name="Boundary Conditions">

    <ParameterList name="LeftBC">
      <Parameter name="Assigned Regions" type="Array(string)" value="{Left}"/>
      <ParameterList name="BC: Hydrostatic">
        <Parameter name="Times" type="Array(double)" value="{0.0, 1.0}"/>
        <Parameter name="Time Functions" type="Array(string)" value="{Constant}"/>
        <Parameter name="Water Table Height" type="Array(double)" value="{20.0, 20.0}"/>
      </ParameterList>
    </ParameterList>

    <ParameterList name="RightBC">
      <Parameter name="Assigned Regions" type="Array(string)" value="{Right}"/>
      <ParameterList name="BC: Hydrostatic">
        <Parameter name="Times" type="Array(double)" value="{0.0, 1.0}"/>
        <Parameter name="Time Functions" type="Array(string)" value="{Constant}"/>
        <Parameter name="Water Table Height" type="Array(double)" value="{19.0, 19.0}"/>
      </ParameterList>
    </ParameterList>

  </ParameterList>

  <ParameterList name="Output">

    <ParameterList name="Time Macros">
      <ParameterList name="Steady State">
	<Parameter name="Values" type="Array(double)" value="{1000.0}"/>
      </ParameterList>
    </ParameterList>

    <ParameterList name="Observation Data">

      <Parameter name="Observation Output Filename" type="string" value="observation.out"/>
      <ParameterList name="Pressure 1f">
        <Parameter name="Region" type="string" value="LeftBoundaryFront"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Aqueous pressure"/>
        <Parameter name="Time Macros" type="Array(string)" value="{Steady State}"/>
      </ParameterList>
      <ParameterList name="Pressure 2f">
        <Parameter name="Region" type="string" value="RightBoundaryFront"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Aqueous pressure"/>
        <Parameter name="Time Macros" type="Array(string)" value="{Steady State}"/>
      </ParameterList>
      <ParameterList name="Pressure 3f">
        <Parameter name="Region" type="string" value="MidpointFront"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Aqueous pressure"/>
        <Parameter name="Time Macros" type="Array(string)" value="{Steady State}"/>
      </ParameterList>
      <ParameterList name="Pressure 4f">
        <Parameter name="Region" type="string" value="LeftmostCellFront"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Aqueous pressure"/>
        <Parameter name="Time Macros" type="Array(string)" value="{Steady State}"/>
      </ParameterList>
      <ParameterList name="Pressure 5f">
        <Parameter name="Region" type="string" value="RightmostCellFront"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Aqueous pressure"/>
        <Parameter name="Time Macros" type="Array(string)" value="{Steady State}"/>
      </ParameterList>
      <ParameterList name="Pressure 6f">
        <Parameter name="Region" type="string" value="LeftMidpointCellFront"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Aqueous pressure"/>
        <Parameter name="Time Macros" type="Array(string)" value="{Steady State}"/>
      </ParameterList>
      <ParameterList name="Pressure 7f">
        <Parameter name="Region" type="string" value="RightMidpointCellFront"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Aqueous pressure"/>
        <Parameter name="Time Macros" type="Array(string)" value="{Steady State}"/>
      </ParameterList>

      <ParameterList name="Pressure 1b">
        <Parameter name="Region" type="string" value="LeftBoundaryBack"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Aqueous pressure"/>
        <Parameter name="Time Macros" type="Array(string)" value="{Steady State}"/>
      </ParameterList>
      <ParameterList name="Pressure 2b">
        <Parameter name="Region" type="string" value="RightBoundaryBack"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Aqueous pressure"/>
        <Parameter name="Time Macros" type="Array(string)" value="{Steady State}"/>
      </ParameterList>
      <ParameterList name="Pressure 3b">
        <Parameter name="Region" type="string" value="MidpointBack"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Aqueous pressure"/>
        <Parameter name="Time Macros" type="Array(string)" value="{Steady State}"/>
      </ParameterList>
      <ParameterList name="Pressure 4b">
        <Parameter name="Region" type="string" value="LeftmostCellBack"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Aqueous pressure"/>
        <Parameter name="Time Macros" type="Array(string)" value="{Steady State}"/>
      </ParameterList>
      <ParameterList name="Pressure 5b">
        <Parameter name="Region" type="string" value="RightmostCellBack"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Aqueous pressure"/>
        <Parameter name="Time Macros" type="Array(string)" value="{Steady State}"/>
      </ParameterList>
      <ParameterList name="Pressure 6b">
        <Parameter name="Region" type="string" value="LeftMidpointCellBack"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Aqueous pressure"/>
        <Parameter name="Time Macros" type="Array(string)" value="{Steady State}"/>
      </ParameterList>
      <ParameterList name="Pressure 7b">
        <Parameter name="Region" type="string" value="RightMidpointCellBack"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="Aqueous pressure"/>
        <Parameter name="Time Macros" type="Array(string)" value="{Steady State}"/>
      </ParameterList>
<!--
      <ParameterList name="Darcy velocity / volumetric flux 3">
        <Parameter name="Region" type="string" value="MidpointCell"/>
        <Parameter name="Functional" type="string" value="Observation Data: Point"/>
        <Parameter name="Variable" type="string" value="X-Aqueous volumetric flux"/>
        <Parameter name="Time Macros" type="Array(string)" value="{Steady State}"/>
      </ParameterList>
-->
    </ParameterList>

    <ParameterList name="Visualization Data">
      <Parameter name="File Name Base" type="string" value="steady-flow"/>
      <Parameter name="File Name Digits" type="int" value="5"/>
      <Parameter name="Time Macros" type="Array(string)" value="{Steady State}"/>
    </ParameterList>

  </ParameterList>

</ParameterList>

